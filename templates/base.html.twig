        </header>

        <section>
        {% block body %}{% endblock %}
        </section>

        <footer class="p-3">
            {% block footer %}
                <div class="container text-center">
                    <h3 class="text-bg-white">Footer</h3>
                </div>
            {% endblock %}
        </footer>
