                            <form class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                                <input type="search" class="form-control form-control-dark" placeholder="Search" aria-label="Search">
                            </form>
                            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                <button type="button" class="btn btn-outline-light mx-2"><a class="text-decoration-none text-dark" href="{{ path ('app_logout')}}">Logout</a></button>
                            {% else %}
                                <button type="button" class="btn btn-outline-light mx-2"><a class="text-decoration-none text-dark" href="{{ path ('app_login')}}">Login</a></button>
